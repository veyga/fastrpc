{
  "adapters": {
    "python-remote-5680": {
      "port": "5680",
      "launch": {
        "remote": {
          "launchCommmand": [
            "python",
            "-m",
            "debugpy",
            "--listen",
            "0.0.0.0:5680",
            "%CMD%"
          ]
        }
      }
    },
    "python-remote-5681": {
      "port": "5681",
      "launch": {
        "remote": {
          "launchCommmand": [
            "python",
            "-m",
            "debugpy",
            "--listen",
            "0.0.0.0:5681",
            "%CMD%"
          ]
        }
      }
    },
    "python-remote-5682": {
      "port": "5682",
      "launch": {
        "remote": {
          "launchCommmand": [
            "python",
            "-m",
            "debugpy",
            "--listen",
            "0.0.0.0:5682",
            "%CMD%"
          ]
        }
      }
    }
  },
  "configurations": {
    "server": {
      "adapter": "python-remote-5680",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "/app"
          }
        ]
      }
    },
    "test": {
      "adapter": "python-remote-5681",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "/app"
          }
        ]
      }
    },
    "script": {
      "adapter": "python-remote-5682",
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "N",
          "userUnhandled": "N"
        }
      },
      "remote-request": "attach",
      "configuration": {
        "request": "attach",
        "justMyCode": true,
        "pathMappings": [
          {
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "/app"
          }
        ]
      }
    }
  }
}
